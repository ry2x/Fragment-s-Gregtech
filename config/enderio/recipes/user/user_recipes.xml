            </grid>
            <output name="BINDER_COMPOSITE" amount="8" />
        </crafting>
    </recipe>

    <recipe name="Conduit Binder" required="true">
        <smelting exp="1.0">
            <input name="BINDER_COMPOSITE" />
            <output name="CONDUIT_BINDER" amount="2" />
        </smelting>
    </recipe>

    <recipe name="Conduit Binder Custom" required="true">
        <smelting exp="1.0">
            <input name="railcraft:reinforced_concrete:*" />
            <output name="CONDUIT_BINDER" amount="9" />
        </smelting>
    </recipe>
